{
  "name": "eslint-config-sanity",
  "version": "7.1.3",
  "description": "Shared eslint configs for sanity-io",
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "eslint-config-sanity",
    "eslint",
    "eslintconfig"
  ],
  "homepage": "https://www.sanity.io/",
  "bugs": {
    "url": "https://github.com/sanity-io/eslint-config-sanity/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/eslint-config-sanity.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "type": "module",
  "main": "index.js",
  "files": [
    "import.js",
    "index.js",
    "react.js",
    "typescript.js"
  ],
  "scripts": {
    "test": "eslint .",
    "inspect": "eslint --inspect-config",
    "inspect:import": "eslint --inspect-config import.js",
    "inspect:react": "eslint --inspect-config react.js",
    "inspect:ts": "eslint --inspect-config typescript.js"
  },
  "prettier": {
    "bracketSpacing": false,
    "plugins": [
      "prettier-plugin-packagejson"
    ],
    "printWidth": 100,
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^2.9.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.11.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^0.5.5",
    "@eslint/js": "^9.13.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "eslint": "^9.13.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.5.3",
    "typescript-eslint": "^8.11.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.5 || ^7.0.0",
    "eslint": "^9.13.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2"
  },
  "peerDependenciesMeta": {
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "eslint-plugin-import": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
